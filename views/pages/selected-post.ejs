            const articleCard = document.querySelector('#single-news');
            // row
            const articleRow = document.createElement('div');
            articleRow.classList.add('row');

            // title and image div
            const articleHeader = document.createElement('div');
            articleHeader.classList.add('col-sm-11', 'grid-margin');

            // title
            const articleTitle = document.createElement('h1')
            articleTitle.classList.add('font-weight-600', 'mb-4');
            articleTitle.innerHTML = singleArticle.data.title;

            // image
            const articleImgGroup = document.createElement('div')
            const articleImg = document.createElement('img')
            articleImg.classList.add('img-fluid');
            articleImg.src = singleArticle.data.image;

            articleImgGroup.appendChild(articleImg);
            articleHeader.appendChild(articleTitle)
            articleHeader.appendChild(articleImgGroup);
            articleRow.appendChild(articleHeader);
            
            //article sub and body 
            const articleBodyGroup = document.createElement('div')
            articleBodyGroup.classList.add('col-sm-10');
            
            //subtitle
            const articleSubtitle = document.createElement('h2');
            articleSubtitle.classList.add('font-weight-600', 'mb-2');
            articleSubtitle.innerHTML = singleArticle.data.subtitle;
            articleBodyGroup.appendChild(articleSubtitle);
            
            // body
            const articleBody = document.createElement('div');
            // articleBody.appendChild(singleArticle.bodyhtml);
            articleBody.innerHTML = singleArticle.data.bodyhtml;
            articleBodyGroup.appendChild(articleBody);
            
            // author
            const articleAuthor = document.createElement('small')
            articleAuthor.classList.add('text-muted');
            articleAuthor.innerHTML = `Author: ${singleArticle.data.author}`;
            articleBodyGroup.appendChild(articleAuthor);
            
            articleRow.appendChild(articleBodyGroup);
            articleCard.appendChild(articleRow);

            }
