    <script>
        const getAllArticles = async () => {
          const response = await fetch('http://localhost:3000/api/news');
          const allArticles = await response.json();
          console.log('eeeee', allArticles.data);
          const cardBody = document.querySelector('#card-body');
          allArticles.data.slice(0,2).map(article => {
            const row = document.createElement('div')
            row.classList.add('row');
            // image elements
            const articleImgGroup = document.createElement('div');
            articleImgGroup.classList.add('col-sm-4', 'grid-margin');
            const articleImgGroupPosition = document.createElement('div');
            articleImgGroupPosition.classList.add('position-relative');
            const articleImgDesign = document.createElement('div')
            articleImgDesign.classList.add('rotate-img');
            const articleImg = document.createElement('img');
            articleImg.classList.add('img-fluid'); 
            const badgePosition = document.createElement('div')
            badgePosition.classList.add('badge-positioned');
            const badge = document.createElement('span');
            badge.innerHTML = article.category;
            badge.classList.add('badge', 'badge-danger', 'font-weight-bold');
            console.log('article', articleImg);
            articleImg.src = article.image;
            articleImgDesign.appendChild(articleImg);
            articleImgGroupPosition.appendChild(articleImgDesign);
            articleImgGroup.appendChild(articleImgGroupPosition);
            // badge
            badgePosition.appendChild(badge);
            articleImgGroupPosition.appendChild(badgePosition);
            row.appendChild(articleImgGroup);

            // article description
            const articleDescGroup = document.createElement('div');
            articleDescGroup.classList.add('col-sm-8', 'grid-margin');
            const articleTitle = document.createElement('h2');
            articleTitle.classList.add('mb-2', 'font-weight-600')

            //a tag to navigate to single blog
            const articleAnchor = document.createElement('a');
            articleAnchor.innerHTML = article.title;
            articleAnchor.href = `http://localhost:3000/api/news/${article.news_id}`;
            articleTitle.appendChild(articleAnchor);

            // article description sliced
            const articleDesc = document.createElement('p');
            articleDesc.classList.add('mb-0');
            articleDesc.innerHTML = `${article.body.slice(0, 150)}...`;
            articleDescGroup.appendChild(articleTitle);
            articleDescGroup.appendChild(articleDesc);
            row.appendChild(articleDescGroup);
            cardBody.appendChild(row);
          });
        }
    </script>
